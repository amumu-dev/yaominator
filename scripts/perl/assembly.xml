<project>
    <actions/>
    <description/>
    <logRotator>
        <daysToKeep>15</daysToKeep>
        <numToKeep>15</numToKeep>
        <artifactDaysToKeep>-1</artifactDaysToKeep>
        <artifactNumToKeep>-1</artifactNumToKeep>
    </logRotator>
    <keepDependencies>false</keepDependencies>
    <properties/>
    <scm class="hudson.scm.SubversionSCM">
        <locations>
            <hudson.scm.SubversionSCM_-ModuleLocation>
                <remote>svn+ssh://svn.corp.yahoo.com/yahoo/media/common/build/trunk/assembly</remote>
                <local>assembly</local>
            </hudson.scm.SubversionSCM_-ModuleLocation>
            <hudson.scm.SubversionSCM_-ModuleLocation>
                <remote>svn+ssh://svn.corp.yahoo.com/yahoo/media/common/build/trunk/dep</remote>
                <local>dep</local>
            </hudson.scm.SubversionSCM_-ModuleLocation>
        </locations>
        <useUpdate>false</useUpdate>
        <doRevert>false</doRevert>
        <browser class="hudson.scm.browsers.ViewSVN">
            <url>http://svn.corp.yahoo.com/view/yahoo/media/common/</url>
        </browser>
        <excludedRegions/>
        <includedRegions/>
        <excludedUsers/>
        <excludedRevprop/>
        <excludedCommitMessages/>
    </scm>
    <assignedNode>globalmedia_build_slaves</assignedNode>
    <canRoam>false</canRoam>
    <disabled>false</disabled>
    <blockBuildWhenUpstreamBuilding>false</blockBuildWhenUpstreamBuilding>
    <triggers class="vector">
        <hudson.triggers.TimerTrigger>
            <spec>0 * * * *</spec>
        </hudson.triggers.TimerTrigger>
    </triggers>
    <concurrentBuild>false</concurrentBuild>
    <builders>
        <com.yahoo.yhudson.tasks.Gmake>
            <makefileLocation>assembly</makefileLocation>
            <alternateMakefile>Makefile</alternateMakefile>
            <targets>
                <com.yahoo.yhudson.tasks.GmakeParam>
                    <param>fe</param>
                </com.yahoo.yhudson.tasks.GmakeParam>
            </targets>
            <options>
                <com.yahoo.yhudson.tasks.GmakeParam>
                    <param/>
                </com.yahoo.yhudson.tasks.GmakeParam>
            </options>
        </com.yahoo.yhudson.tasks.Gmake>
    </builders>
    <publishers>
        <hudson.tasks.test.AggregatedTestResultPublisher/>
        <hudson.tasks.Fingerprinter>
            <targets>dep/mlb_rhel5_yinst7_dev.state </targets>
            <recordBuildArtifacts>false</recordBuildArtifacts>
        </hudson.tasks.Fingerprinter>
        <hudson.tasks.BuildTrigger>
            <childProjects>globalmedia-fe_lists-trunk-test-smoke-int1, globalmedia-fe_article-trunk-test-smoke-int1, globalmedia-fe_baseplate-trunk-test-smoke-int1, globalmedia-fe_video-trunk-test-smoke-int1, globalmedia-fe_photos-trunk-test-smoke-int1, globalmedia-fe_blogs-trunk-test-smoke-int1, globalmedia-fe_social-trunk-test-smoke-int1</childProjects>
            <threshold>
                <name>SUCCESS</name>
                <ordinal>0</ordinal>
                <color>BLUE</color>
            </threshold>
        </hudson.tasks.BuildTrigger>
        <join.JoinTrigger>
            <joinProjects>utility-fe-trunk-promote-Int1ToQa1-auto</joinProjects>
            <joinPublishers/>
            <evenIfDownstreamUnstable>false</evenIfDownstreamUnstable>
        </join.JoinTrigger>
        <hudson.tasks.Mailer>
            <recipients>lego-build@yahoo-inc.com</recipients>
            <dontNotifyEveryUnstableBuild>false</dontNotifyEveryUnstableBuild>
            <sendToIndividuals>false</sendToIndividuals>
        </hudson.tasks.Mailer>
        <hudson.plugins.ircbot.IrcPublisher>
            <targets class="java.util.Collections$EmptyList"/>
            <strategy>FAILURE_AND_FIXED</strategy>
            <notifyOnBuildStart>false</notifyOnBuildStart>
            <notifySuspects>false</notifySuspects>
            <notifyCulprits>false</notifyCulprits>
            <notifyFixers>false</notifyFixers>
            <notifyUpstreamCommitters>false</notifyUpstreamCommitters>
            <buildToChatNotifier class="hudson.plugins.im.build_notify.SummaryOnlyBuildToChatNotifier"/>
            <channels/>
        </hudson.plugins.ircbot.IrcPublisher>
    </publishers>
    <style/>
    <buildWrappers>
        <com.yahoo.yhudson.BuildyBlockBuildWrapper>
            <pathToBim>assembly/build/build-assemble.bim</pathToBim>
            <deletionPolicy>FALSE</deletionPolicy>
        </com.yahoo.yhudson.BuildyBlockBuildWrapper>
        <hudson.plugins.locksandlatches.LockWrapper>
            <locks>
                <hudson.plugins.locksandlatches.LockWrapper_-LockWaitConfig>
                    <name>lock_globalmedia-fe_baseplate-trunk-test-smoke-int1</name>
