                    <name>lock_globalmedia-fe_lists-trunk-test-smoke-int1</name>
                </hudson.plugins.locksandlatches.LockWrapper_-LockWaitConfig>
                <hudson.plugins.locksandlatches.LockWrapper_-LockWaitConfig>
                    <name>lock_globalmedia-fe_social-trunk-test-smoke-int1</name>
